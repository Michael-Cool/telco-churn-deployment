{
  "id": null,
  "uid": "fastapi-local",
  "title": "FastAPI Monitoring (Local)",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 5,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests pro Sekunde",
      "id": 1,
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "rate(http_requests_total[1m])",
          "legendFormat": "{{handler}} ({{status}})",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Durchschnittliche Latenz (Sekunden)",
      "id": 2,
      "gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "rate(http_request_duration_seconds_sum[1m]) / rate(http_request_duration_seconds_count[1m])",
          "legendFormat": "{{handler}}",
          "refId": "B"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Fehlerquote (alle Fehler in %)",
      "id": 3,
      "gridPos": { "x": 0, "y": 16, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "100 * (sum(rate(http_requests_total{status!~\"2..\"}[1m])) / sum(rate(http_requests_total[1m])))",
          "legendFormat": "Fehlerquote (4xx + 5xx)",
          "refId": "C"
        }
      ]
    }
  ]
}