{
  "id": null,
  "title": "Churn API Monitoring Dashboard (9100)",
  "tags": ["fastapi", "prometheus", "mlops"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "üìà Requests per Second",
      "targets": [
        {
          "expr": "rate(http_requests_total[1m])",
          "legendFormat": "{{handler}} ({{method}} {{status}})"
        }
      ],
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8}
    },
    {
      "type": "timeseries",
      "title": "‚ö° Response Time (95th Percentile)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "p95 latency"
        }
      ],
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "‚ùå Error Rate (%)",
      "targets": [
        {
          "expr": "100 * (sum(rate(http_requests_total{status!~\"2..\"}[1m])) / sum(rate(http_requests_total[1m])))",
          "legendFormat": "error rate"
        }
      ],
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    }
  ],
  "time": {"from": "now-15m", "to": "now"},
  "timepicker": {"refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"]}
}